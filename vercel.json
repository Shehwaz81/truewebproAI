{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/generate",
      "dest": "/api/generate"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
